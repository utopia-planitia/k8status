{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:base"
    ],
    "pinDigests": true,
    "postUpdateOptions": [
      "gomodTidy",
      "gomodMassage",
      "gomodUpdateImportPaths"
    ],
    "prHourlyLimit": 0,
    "labels": [
      "renovate-bot"
    ],
    "automerge": true,
    "automergeType": "pr",
    "platformAutomerge": true,
    "postUpgradeTasks": {
      "commands": [],
      "fileFilters": [
        "**/**"
      ]
    },
    "separateMinorPatch": true,
    "dependencyDashboard": false
  }
  
  